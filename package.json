{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "NODE_ENV=production npm run build:next && npm run build:server",
    "build:next": "next build",
    "build:server": "tsc --project server/tsconfig.json && node ./build-tools/ts-alias.js",
    "start": "NODE_ENV=production node ./dist/server/index.js",
    "test": "npm run test-only && npm run lint",
    "test-only": "jest",
    "test-debug": "node --inspect node_modules/jest/bin/jest.js --watch --runInBand",
    "lint": "npm run lint-nextjs && npm run lint-custom-server",
    "lint-nextjs": "tslint -p tsconfig.json -c tslint.yaml",
    "lint-custom-server": "tslint -p server/tsconfig.json -c tslint.yaml",
    "prettier": "prettier --write '**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "chalk": "^4.1.1",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "express": "^4.17.1",
    "next": "^11.0.1",
    "next-i18next": "^8.5.3",
    "next-redux-wrapper": "^7.0.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "uid-safe": "^2.1.5",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@next/bundle-analyzer": "^10.2.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cookie-session": "^2.0.42",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/next-redux-wrapper": "^3.0.0",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.33",
    "@types/passport-twitter": "^1.0.36",
    "@types/react-redux": "^7.1.16",
    "@types/redux-immutable-state-invariant": "^2.1.1",
    "@types/sinon": "^10.0.0",
    "@types/uid-safe": "^2.1.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "^1.13.3",
    "file-loader": "^6.2.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "sass": "^1.34.1",
    "sinon": "^10.0.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "tslint-loader": "^3.5.4",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1"
  }
}
