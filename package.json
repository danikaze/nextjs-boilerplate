{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "tslint -p tsconfig.json -c tslint.yaml",
    "prettier": "prettier --write '**/*.{ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "next": "9.5.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "git-revision-webpack-plugin": "^3.0.6",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "tslint": "^6.1.3",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "tslint-loader": "^3.5.4",
    "tslint-microsoft-contrib": "^6.2.0",
    "tslint-react": "^5.0.0"
  }
}
